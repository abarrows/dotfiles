{
  "build": {
    "args": {
      "NODE_VERSION": "lts/*",
      "UPGRADE_PACKAGES": "false",
      "VARIANT": "2.6",
      "WORKSPACE": "/workspace/"
    },
    "dockerfile": "ruby.Dockerfile",
    "workspaceFolder": "/workspace"
  },
  "extensions": [
	"aliariff.vscode-erb-beautify",
	"ms-vsonline.vsonline",
	"castwide.solargraph",
	"pivotal.vscode-manifest-yaml",
	"rebornix.ruby",
	"redhat.vscode-yaml",
	"vortizhe.simple-ruby-erb",
	"wingrunr21.vscode-ruby",
	"bmalehorn.shell-syntax",
	"timonwong.shellcheck"
],
  "forwardPorts": [
    3060
  ],
  "name": "AB: Containerized Development - Ruby on Rails",
  "postCreateCommand": "echo Welcome to your cloud machine.  Get to Work!",
  "remoteUser": "vscode",
  "runArgs": [
    "--env-file",
    "${localWorkspaceFolder}/devcontainer.env"
  ],
  "settings": {
    "[javascript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[ruby]": {
      "editor.defaultFormatter": "castwide.solargraph",
      "editor.formatOnSave": true
    },
    "[yaml]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "atlascode.bitbucket.enabled": false,
    "atlascode.jira.enabled": true,
    "atlascode.jira.explorer.assignedIssueJql": "assignee=currentUser() and resolution = Unresolved",
    "atlascode.jira.explorer.enabled": true,
    "atlascode.jira.explorer.nestSubtasks": true,
    "atlascode.jira.jqlList": [
      {
        "enabled": true,
        "id": "faedfb95-7fef-47c3-a54b-cc397464edf9",
        "monitor": true,
        "name": "Content Admin - My Issues",
        "query": "project = \"CONADMIN\" and assignee = currentUser() ",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      },
      {
        "enabled": true,
        "id": "110bea68-bd07-43aa-b1bc-2512556cd473",
        "monitor": true,
        "name": "TPS - My Issues",
        "query": "project in (\"PUZSOC\",\"GAMES\") AND status != resolved and assignee = currentUser() ORDER by rank ASC, priority DESC, updatedDate DESC",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      },
      {
        "enabled": true,
        "id": "02142ed5-c4d7-41af-a082-7e16cb33dada",
        "monitor": true,
        "name": "Games/Embed - Issues",
        "query": "project in (Games, EMBEDSERV) AND status != resolved and assignee = currentUser() ORDER by rank ASC, priority DESC, updatedDate DESC",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      },
      {
        "enabled": true,
        "id": "a41f9986-a92a-4936-93f3-e653e95f2ae7",
        "monitor": true,
        "name": "Doonesbury - My Issues",
        "query": "project = \"DOONES\" AND status != resolved and assignee = currentUser() ORDER by rank ASC, priority DESC, updatedDate DESC",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      },
      {
        "enabled": true,
        "id": "486d46cd-29a5-47e7-b5cb-7bdf9625be72",
        "monitor": true,
        "name": "My amuniversal Issues",
        "query": "assignee = currentUser() and status != resolved ORDER BY rank ASC, priority DESC",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      },
      {
        "enabled": true,
        "id": "6fbe3fdb-a368-413f-a5ca-5a1692a2892e",
        "monitor": true,
        "name": "GoComics - My Issues",
        "query": "project = GoComics AND status != resolved and assignee = currentUser() ORDER by rank ASC, priority DESC, updatedDate DESC",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      },
      {
        "enabled": true,
        "id": "1b3ae036-7577-419f-9d72-393c91180dff",
        "monitor": true,
        "name": "Uexpress- My Issues",
        "query": "project = Uexpress AND status != resolved AND assignee = currentUser() ",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      },
      {
        "enabled": true,
        "id": "dbf335f1-eb46-42b5-b25e-2f357b6ab7a1",
        "monitor": true,
        "name": "Dilbert - My Issues",
        "query": "project = Dilbert AND status != resolved and assignee = currentUser() ORDER by rank ASC, priority DESC, updatedDate DESC",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      },
      {
        "enabled": true,
        "id": "c33593e5-febe-4787-adce-44be66d943ab",
        "monitor": true,
        "name": "My Most Recently Created Issues",
        "query": "reporter = currentUser() AND status != Done ORDER BY updatedDate",
        "siteId": "6002c11a-0ab3-4e92-b2d1-eb97960ab7a6"
      }
    ],
